{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka.ProducerConfig": {
    "TopicName": "<ENTER-TOPIC-NAME-HERE>",

    "BootstrapServers": "<IPS-FOR-BOOTSTRAP-SERVERS>:9092",
    "ClientId": "<CLIENT-ID>",

    "Acks": "All",
    "MessageTimeoutMs": 3000,
    "BatchNumMessages": 10000,
    "LingerMs": 5,
    "CompressionType": "Snappy",

    "EnableIdempotence": true,

    "MessageSendMaxRetries": 100,
    "MaxInFlight": 5
  }
}